
import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;
import java.util.Arrays;
import java.util.Collections;
import java.util.Comparator;
import java.util.StringTokenizer;

public class Main {
	static String[] input;
	//static int[] sum;
	public static void main(String[] args) throws NumberFormatException, IOException {
		BufferedReader br = new BufferedReader(new InputStreamReader(System.in)); 
		int n = Integer.parseInt(br.readLine());
		StringTokenizer st;
		
		
		String[] result = new String[n];
	//	sum = new int[n]; //글자 내 숫자의 합
		input = new String[n];
		//입력 받기 
		for(int i=0;i<n;i++) {
			st = new StringTokenizer(br.readLine());
			input[i] = st.nextToken();
			
		
		}
		//길이 순으로 정렬 함 
		Arrays.sort(input,new sComparator());
		for(String i: input) System.out.println(i);
		//배열마다 숫자유무판단/숫자 있으
		
		
		
		
	}
	
	public static class sComparator implements Comparator<String>{
		
		@Override
		public int compare(String o1, String o2) {
			if(o1.length()>o2.length()) return 1; //바꾼다. 
			else if(o1.length()==o2.length()) {
				//숫자 합까지 같으면 그 때 사전순 
				if(count(o1,o2) == 0 ) return o1.compareTo(o2);
				else return count(o1,o2); //길이가 같으면 숫자의 합으로 비교
			}
			return -1; //그냥 고정
		}
		//return o1.length() - o2.length(); 오름차순
		//return o2.length() - o1.length(); 내림차순
		

	}
	
	public static int count(String o1,String o2) {
		int o1Sum = 0 ;
		int o2Sum =0;
		
		for(int j=0;j<o1.length();j++) {
			char now = o1.charAt(j) ;
			if(now>= '0' && now <= '9') {
				o1Sum += now-'0';
			}
		}
		
		for(int j=0;j<o2.length();j++) {
			char now = o2.charAt(j) ;
			if(now>= '0' && now <= '9') {
				o2Sum += now-'0';
			}
		}
		return o1Sum - o2Sum;
	}
	

	
	

}
